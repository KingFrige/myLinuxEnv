#%Module -*- tcl -*-

conflict         gcc/gcc-9.3.0
conflict         gcc/gcc-12.3.0
conflict         gcc/gcc-13.2.0

set              tool_home       /home/korben/tools/gcc/gcc-12.2.0
setenv           CC              $tool_home/bin/gcc
setenv           CXX             $tool_home/bin/g++

prepend-path     PATH            $tool_home/bin
prepend-path     LIBRARY_PATH    $tool_home/lib
prepend-path     LIBRARY_PATH    $tool_home/lib64
prepend-path     LD_LIBRARY_PATH $tool_home/lib
prepend-path     LD_LIBRARY_PATH $tool_home/lib64
prepend-path     LD_INCLUDE_PATH $tool_home/include
prepend-path     MANPATH         $tool_home/share/man
